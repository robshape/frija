{
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  },
  "extends": "airbnb",
  "globals": {
    "artifacts": true
  },
  "plugins": [
    "react-hooks"
  ],
  "rules": {
    "jsx-a11y/label-has-for": {
      "allowChildren": true
    },
    "react-hooks/exhaustive-deps": "warn",
    "react-hooks/rules-of-hooks": "error"
  }
}
