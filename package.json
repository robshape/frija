{
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "7.0.0",
    "@testing-library/jest-dom": "4.1.0",
    "@testing-library/react": "9.1.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "cache-loader": "4.1.0",
    "core-js": "3.2.1",
    "css-loader": "3.2.0",
    "dotenv-webpack": "1.7.0",
    "eslint": "6.3.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "2.0.1",
    "ethlint": "1.2.4",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "node-sass": "4.12.0",
    "nodemon": "1.19.1",
    "prop-types": "15.7.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-hot-loader": "4.12.12",
    "react-router-dom": "5.0.1",
    "regenerator-runtime": "0.13.3",
    "sass-loader": "8.0.0",
    "style-loader": "1.0.0",
    "stylelint": "10.1.0",
    "stylelint-config-standard": "18.3.0",
    "stylelint-scss": "3.10.0",
    "truffle": "5.0.34",
    "uuid": "3.3.3",
    "webpack": "4.39.3",
    "webpack-cli": "3.3.7",
    "webpack-dev-server": "3.8.0"
  },
  "private": true,
  "scripts": {
    "build:ethereum": "(cd ./src/ethereum/ && truffle compile --all)",
    "deploy:ethereum": "(cd ./src/ethereum/ && truffle migrate --reset)",
    "lint": "npm run lint:js && npm run lint:sass && npm run lint:sol",
    "lint:js": "eslint --ext .js,.jsx ./",
    "lint:sass": "stylelint './src/client/**/*.scss'",
    "lint:sol": "solium --dir ./src/ethereum/contracts/ --no-soliumignore",
    "run:client": "webpack-dev-server --progress",
    "run:ethereum": "(cd ./src/ethereum/ && truffle develop)",
    "run:server": "nodemon --watch ./src/server/ ./src/server/index.js",
    "setup": "./scripts/setup-project.sh",
    "start": "npm run run:server & npm run run:client",
    "test": "jest --config ./tests/jest.config.js",
    "test:coverage": "npm test -- --coverage",
    "test:watch": "npm test -- --watch"
  }
}
