{
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.2.0",
    "@babel/preset-react": "7.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "css-loader": "2.0.1",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "enzyme-to-json": "3.3.5",
    "eslint": "5.10.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.0.0-alpha.9",
    "node-sass": "4.11.0",
    "nodemon": "1.18.9",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-hot-loader": "4.6.3",
    "sass-loader": "7.1.0",
    "solium": "1.1.8",
    "style-loader": "0.23.1",
    "stylelint": "9.9.0",
    "stylelint-config-standard": "18.2.0",
    "truffle": "5.0.0",
    "webpack": "4.28.0",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10"
  },
  "private": true,
  "scripts": {
    "build:ethereum": "(cd ethereum && truffle compile --all)",
    "deploy:ethereum": "(cd ethereum && truffle migrate --reset)",
    "install:client": "npm --prefix client install",
    "install:server": "npm --prefix server install",
    "lint": "npm run lint:js && npm run lint:sass && npm run lint:sol",
    "lint:js": "eslint --ext .js,.jsx ./",
    "lint:sass": "stylelint './client/**/*.scss'",
    "lint:sol": "solium --dir ./ethereum/contracts",
    "postinstall": "npm run install:client && npm run install:server",
    "run:client": "webpack-dev-server",
    "run:ethereum": "(cd ethereum && truffle develop)",
    "run:server": "nodemon --watch server ./server/index.js",
    "start": "npm run run:server & npm run run:client",
    "test": "jest --config ./tests/jest.config.js"
  }
}
